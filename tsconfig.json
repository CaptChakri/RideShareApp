{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "baseUrl": ".",
    "paths": {
      "constants/*": [
        "constants/*"
      ],
      "screens/*": [
        "screens/*"
      ],
      "assets/*": [
        "assets/*"
      ],
      "components/*": [
        "components/*"
      ],
      "types/*": [
        "types/*"
      ]
    },
    "noImplicitAny": false,
    "skipLibCheck": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "types/**/*.d.ts"
  ]
}
